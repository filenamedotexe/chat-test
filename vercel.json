{
  "framework": "nextjs",
  "buildCommand": "cd app && npm run build",
  "devCommand": "cd app && npm run dev",
  "installCommand": "npm install",
  "outputDirectory": "app/.next",
  "git": {
    "deploymentEnabled": {
      "main": true,
      "editing-branch": true
    }
  },
  "github": {
    "autoAlias": false
  },
  "functions": {
    "app/app/api/chat-langchain/route.ts": {
      "maxDuration": 60
    },
    "app/app/api/user/*/route.ts": {
      "maxDuration": 30
    }
  }
}